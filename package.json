{
  "name": "waves",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "vercel-build": "expo export --platform web",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "concurrently -n convex,expo -c cyan,green \"npm:convex:dev\" \"npm:start\"",
    "dev:server": "concurrently -n convex,expo -c cyan,green \"npm:convex:dev:portable\" \"npm:web:server\"",
    "dev:prime": "npm run convex:seed && npm run dev",
    "dev:server:prime": "npm run convex:seed:portable && npm run dev:server",
    "typecheck": "tsc --noEmit",
    "convex:dev": "PATH=\"/opt/homebrew/opt/node@22/bin:$PATH\" convex dev --typecheck disable",
    "convex:dev:portable": "npx convex dev --typecheck disable",
    "convex:seed": "PATH=\"/opt/homebrew/opt/node@22/bin:$PATH\" convex dev --once --typecheck disable --run internal.seed.seedDemoData",
    "convex:seed:portable": "npx convex dev --once --typecheck disable --run internal.seed.seedDemoData",
    "web:server": "npx expo start --web --host lan"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.22",
    "@expo/vector-icons": "^15.0.3",
    "@radix-ui/react-popover": "^1.1.15",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.8",
    "convex": "^1.31.7",
    "date-fns": "^4.1.0",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "pigeon-maps": "^0.22.1",
    "react": "19.1.0",
    "react-day-picker": "^9.13.2",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
